  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "prettier": "^2.8.7",
    "prettier-plugin-astro": "^0.10.0",
    "typescript": "^5.0.2"
  }
