{
  "name": "site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx,.astro && prettier --check --plugin-search-dir=. .",
    "check-no-prettier": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "ci": "pnpm run check-no-prettier && pnpm run build"
  },
  "dependencies": {
    "@astrojs/tailwind": "5.0.2",
    "astro": "3.6.0",
    "logrocket": "8.1.0",
    "tailwindcss": "3.3.5"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "0.33.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "typescript": "5.4.3"
  }
}
