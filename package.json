{
  "name": "site",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx,.astro && prettier --check --plugin-search-dir=. .",
    "check-no-prettier": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "ci": "pnpm run check-no-prettier && pnpm run build"
  },
  "dependencies": {
    "@astrojs/tailwind": "5.0.2",
    "astro": "3.4.0",
    "logrocket": "6.0.1",
    "tailwindcss": "3.3.5"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-astro": "0.29.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "typescript": "5.2.2"
  }
}
